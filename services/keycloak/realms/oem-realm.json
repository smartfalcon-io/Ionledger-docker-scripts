{
  "realm": "oem",
  "enabled": true,
  "roles": {
    "realm": [
      { "name": "admin", "description": "Administrator role" },
      { "name": "issuer", "description": "Issuer role" },
      { "name": "user", "description": "Regular user role" }
    ]
  },
  "clients": [
    {
      "clientId": "oem-api",
      "enabled": true,
      "publicClient": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "protocol": "openid-connect",
      "clientAuthenticatorType": "client-secret",
      "secret": "oem-api-secret",
      "redirectUris": ["*"],
      "webOrigins": ["*"]
    },
    {
      "clientId": "oem-web",
      "enabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "redirectUris": ["*"],
      "webOrigins": ["*"]
    }
  ],
  "users": [
    {
      "username": "oem_admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "OEM",
      "lastName": "Admin",
      "email": "oem_admin@example.com",
      "credentials": [
        { "type": "password", "value": "oem123", "temporary": false }
      ],
      "realmRoles": ["admin", "issuer"]
    }
  ]
}
