{
  "realm": "user",
  "enabled": true,
  "roles": {
    "realm": [
      { "name": "user", "description": "Regular user role" },
      { "name": "admin", "description": "Administrator role" }
    ]
  },
  "clients": [
    {
      "clientId": "user-api",
      "enabled": true,
      "publicClient": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "protocol": "openid-connect",
      "clientAuthenticatorType": "client-secret",
      "secret": "user-api-secret",
      "redirectUris": ["*"],
      "webOrigins": ["*"]
    },
    {
      "clientId": "user-web",
      "enabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "redirectUris": ["*"],
      "webOrigins": ["*"]
    }
  ],
  "users": [
    {
      "username": "user1",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Regular",
      "lastName": "User",
      "email": "user1@example.com",
      "credentials": [
        { "type": "password", "value": "user123", "temporary": false }
      ],
      "realmRoles": ["user","admin"]
    }
  ]
}
